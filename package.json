{
	"name": "react-spring-rocket",
	"version": "0.0.0",
	"private": true,
	"homepage": "./",
	"scripts": {
		"prepare": "husky install",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"style": "prettier --write package.json '**/*{.ts,.tsx,.js,.jsx,.json,.html,.css,.scss,.sass,.yaml,.yml,.md}'",
		"style:ci": "prettier --list-different package.json '**/*{.ts,.tsx,.js,.jsx,.json,.html,.css,.scss,.sass,.yaml,.yml,.md}'",
		"lint": "eslint src/**/*{.ts,.tsx,.js,.jsx} --ignore-pattern node_modules/ --no-error-on-unmatched-pattern",
		"eject": "react-scripts eject"
	},
	"dependencies": {
		"@react-spring/web": "^9.2.3",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-use-gesture": "^9.1.3"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"@types/jest": "^26.0.15",
		"@types/node": "^15.12.2",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"husky": "^6.0.0",
		"prettier": "^2.3.1",
		"pretty-quick": "^3.1.1",
		"react-scripts": "4.0.3",
		"typescript": "^4.3.4"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest",
			"plugin:prettier/recommended",
			"prettier"
		],
		"plugins": [
			"prettier"
		],
		"rules": {
			"prettier/prettier": "warn"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
