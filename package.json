{
	"name": "react-spring-rocket",
	"description": "A simple rocket. Made with React Spring.",
	"version": "0.2.0",
	"license": "MIT",
	"private": true,
	"homepage": "./",
	"engines": {
		"node": ">=14",
		"npm": ">=7"
	},
	"scripts": {
		"prepare": "npm run meta:register-precommit",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"style": "prettier --write package.json '**/*{.ts,.tsx,.js,.jsx,.json,.html,.css,.scss,.sass,.yaml,.yml,.md}'",
		"style:ci": "prettier --list-different package.json '**/*{.ts,.tsx,.js,.jsx,.json,.html,.css,.scss,.sass,.yaml,.yml,.md}'",
		"lint": "eslint src/**/*{.ts,.tsx,.js,.jsx} --ignore-pattern node_modules/ --no-error-on-unmatched-pattern",
		"eject": "react-scripts eject",
		"meta:register-precommit": "node scripts/register-precommit-hook.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fussel178/react-spring-rocket.git"
	},
	"bugs": {
		"url": "https://github.com/fussel178/react-spring-rocket/issues"
	},
	"author": {
		"name": "fussel178",
		"email": "richter@fliegwerk.com"
	},
	"contributors": [
		{
			"name": "Pablo Klaschka",
			"email": "contact@pabloklaschka.de",
			"url": "https://github.com/pklaschka"
		}
	],
	"dependencies": {
		"@react-spring/web": "^9.4.4",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-use-gesture": "^9.1.3"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.16.3",
		"@testing-library/react": "^12.1.4",
		"@testing-library/user-event": "^14.0.4",
		"@types/jest": "^27.4.1",
		"@types/node": "^17.0.23",
		"@types/react": "^17.0.43",
		"@types/react-dom": "^17.0.14",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^2.6.2",
		"pretty-quick": "^3.1.3",
		"react-scripts": "5.0.0",
		"typescript": "^4.6.3"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest",
			"plugin:prettier/recommended",
			"prettier"
		],
		"plugins": [
			"prettier"
		],
		"rules": {
			"prettier/prettier": "warn"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
